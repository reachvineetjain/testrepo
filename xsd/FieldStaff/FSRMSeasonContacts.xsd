         <xsd:element name="rdFirstName" type="xsd:string" minOccurs="1" maxOccurs="1"/>
         <xsd:element name="rdLastName" type="xsd:string" minOccurs="1" maxOccurs="1" />
         <xsd:element name="erdPicture" type="xsd:string" minOccurs="1" maxOccurs="1" />
         <xsd:element name="erdFirstName" type="xsd:string" minOccurs="1" maxOccurs="1"/>
         <xsd:element name="erdLastName" type="xsd:string" minOccurs="1" maxOccurs="1" />
      </xsd:sequence>
   </xsd:complexType>

</xsd:schema>


	               <xsd:element name="hspParticipantEltisRequired" type="xsd:boolean" minOccurs="1" maxOccurs="1" />
	               <xsd:element name="aypParticipantTranscriptRequired" type="xsd:boolean" minOccurs="1" maxOccurs="1" />
	             </xsd:sequence>
	   </xsd:complexType>
	   
	   

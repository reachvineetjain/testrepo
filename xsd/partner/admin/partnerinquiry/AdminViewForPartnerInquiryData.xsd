	               <xsd:element name="note"    type="xsd:string" minOccurs="1" maxOccurs="1" />
	             </xsd:sequence>
	   </xsd:complexType>
	   
	    <xsd:complexType name="PartnerRecruitmentAdminScreeningDocuments">
	     <xsd:complexContent>
     <xsd:extension base="common:Response">
	   <xsd:sequence>
	   <xsd:element name="goId" type="xsd:int" minOccurs="1" maxOccurs="1" />
	    <xsd:element name="partnerDocumentId" type="xsd:int" minOccurs="1" maxOccurs="1" />

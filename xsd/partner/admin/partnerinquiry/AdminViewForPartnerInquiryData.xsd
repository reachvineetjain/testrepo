	               <xsd:element name="fax"    type="xsd:string" minOccurs="1" maxOccurs="1" />
	               <xsd:element name="skypeId"  type="xsd:string" minOccurs="1" maxOccurs="1" />
	               <xsd:element name="username"  type="xsd:string" minOccurs="1" maxOccurs="1" />
	               <xsd:element name="active"  type="xsd:boolean" minOccurs="1" maxOccurs="1" />
	               <xsd:element name="programs"  type="xsd:string" minOccurs="1" maxOccurs="1" />
					<xsd:element name="primaryContact"  type="xsd:boolean" minOccurs="1" maxOccurs="1" />	               
	             </xsd:sequence>
	   </xsd:complexType>
	   
	   
	   <xsd:complexType name="PartnerRecruitmentAdminScreeningReferenceCheck">
	            <xsd:sequence>
	            <xsd:element name="partnerCheckReferenceId" type="xsd:int" minOccurs="1" maxOccurs="1" />
	               <xsd:element name="completedOn" type="xsd:string" minOccurs="1" maxOccurs="1" />
	               <xsd:element name="completedBy" type="xsd:string" minOccurs="1" maxOccurs="1" />

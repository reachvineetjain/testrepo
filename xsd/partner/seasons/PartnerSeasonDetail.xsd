         <xsd:element name="partnerSeasonNoteTitle" type="xsd:string" minOccurs="1" maxOccurs="1" />
         <xsd:element name="partnerSeasonNote" type="xsd:string" minOccurs="0" maxOccurs="1" />
         <xsd:element name="addedByPicUrl" type="xsd:string" minOccurs="1" maxOccurs="1" />
         <xsd:element name="addedBy" type="xsd:string" minOccurs="1" maxOccurs="1" />
         <xsd:element name="addedOn" type="xsd:string" minOccurs="1" maxOccurs="1" />
         <xsd:element name="privacy" type="xsd:string" minOccurs="1" maxOccurs="1" />
      </xsd:sequence>
   </xsd:complexType>

</xsd:schema>
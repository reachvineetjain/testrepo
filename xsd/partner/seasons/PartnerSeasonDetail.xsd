      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="PartnerSeasonNote">
      <xsd:sequence>
         <xsd:element name="partnerSeasonId" type="xsd:int" minOccurs="1" maxOccurs="1" />

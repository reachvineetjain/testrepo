   <xsd:annotation>
      <xsd:appinfo>
         <jaxb:schemaBindings>
            <jaxb:package name="com.ccighgo.service.transport.season.beans.seasonwpdetails" />
         </jaxb:schemaBindings>
      </xsd:appinfo>
      <xsd:documentation></xsd:documentation>
   </xsd:annotation>

   <xsd:element name="SeasonWPDetails" type="wp:SeasonWPDetails"></xsd:element>

   <xsd:complexType name="SeasonWPDetails">
      <xsd:sequence>
         <xsd:element name="seasonId" type="xsd:int" minOccurs="1" maxOccurs="1" />
         <xsd:element name="seasonProgramId" type="xsd:int" minOccurs="1" maxOccurs="1" />
         <xsd:element name="wpBasicDetail" type="wp:WPBasicDetail" minOccurs="1" maxOccurs="1" />
         <xsd:element name="wpSectionOne" type="wp:WPSectionOne" minOccurs="1" maxOccurs="1" />
         <xsd:element name="wpProgramAllocations" type="wp:WPProgramAllocations" minOccurs="1" maxOccurs="1" />
         <xsd:element name="wpDocuments" type="wp:WPDocuments" minOccurs="0" maxOccurs="unbounded" />
         <xsd:element name="wpNotes" type="wp:WPNotes" minOccurs="0" maxOccurs="unbounded" />
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="WPBasicDetail">
      <xsd:sequence>
         <xsd:element name="seasonId" type="xsd:int" minOccurs="1" maxOccurs="1" />
         <xsd:element name="seasonProgramId" type="xsd:int" minOccurs="1" maxOccurs="1" />
         <xsd:element name="programName" type="xsd:string" minOccurs="0" maxOccurs="1" />
         <xsd:element name="programStatus" type="xsd:string" minOccurs="0" maxOccurs="1" />
      </xsd:sequence>
   </xsd:complexType>

   <xsd:complexType name="WPSectionOne">
      <xsd:sequence>
         <xsd:element name="seasonId" type="xsd:int" minOccurs="1" maxOccurs="1" />

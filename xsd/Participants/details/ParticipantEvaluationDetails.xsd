               <xsd:element name="applicationStatus" type="xsd:string" minOccurs="1" maxOccurs="1" />
               <xsd:element name="placementStatus" type="xsd:string" minOccurs="1" maxOccurs="1" />
               <xsd:element name="evaluationDetails" type="p:EvaluationDetails" minOccurs="1" maxOccurs="1" />
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="EvaluationDetails">
      <xsd:sequence>
         <xsd:element name="febEvalCompleted" type="xsd:boolean" minOccurs="1" maxOccurs="1" />
         <xsd:element name="febEvalLastActionDate" type="xsd:string" minOccurs="1" maxOccurs="1" />
         <xsd:element name="octEvalCompleted" type="xsd:boolean" minOccurs="1" maxOccurs="1" />
         <xsd:element name="octEvalLastActionDate" type="xsd:string" minOccurs="1" maxOccurs="1" />
         <xsd:element name="finalEvalCompleted" type="xsd:boolean" minOccurs="1" maxOccurs="1" />

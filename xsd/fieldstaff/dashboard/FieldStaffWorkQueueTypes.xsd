<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.ccighgo.com/fieldstaffworkqueuetype" elementFormDefault="unqualified" attributeFormDefault="unqualified" jaxb:version="2.0"
